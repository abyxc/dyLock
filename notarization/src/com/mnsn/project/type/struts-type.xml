<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="typeAction" namespace="/manage/type" extends="global">
		<action name="*" class="typeAction" method="{1}">
			<result name="list">list.jsp</result>
			<result name="listpapers">listpapers.jsp</result>
			<result name="uppapers">uppapers.jsp</result>
			<result name="tolist" type="redirect">list.action</result>
            <result name="oper">oper.jsp</result>
            <result name="toupdate">oper.jsp</result>
		</action>
	</package>
</struts>